program loopTest is
    integer a;
    integer b;
    bool c;
    begin
        for (a := 0; a<10)
            a := a+1;
            if(a <= 5) then
                putInteger(a);
            end if;
        end for;
        putInteger(a);
        for (b := 0; b<10)
            b := b+2;
            if(b <= 5) then
                putInteger(b);
            else
                putInteger(a*b);
            end if;
            c := 1;
            c := a == c;
            putBool(c);
        end for;
        putInteger(b);
    end program
