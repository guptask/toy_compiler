program test is
    global integer testing[3];
    global string aString;
    integer a;
    integer b;
    integer f;
    integer d;
    global integer c;

    procedure newProc1(integer d in, integer b out)
        integer a[10];
        begin
            a[1+1] := d;
            return;
        end procedure;

    global procedure newProc(integer d in, bool b out)
        integer f[10];
        procedure newProc1(integer d in)
            integer f[10];
            begin
                testing[1+1] := c;
                newProc1(f[2]);
                return;
            end procedure;
        begin
            testing[1+1] := c;
            newProc1(f[2]);
            return;
        end procedure;

    begin
        aString := "Testing...";
        testing[1] := (a * -b) + c*d + f;

        // if statement
        if ( true ) then
            testing[2+a] := a/b;
        else
            testing[2] := a*b;
        end if;

        // for statement
        for (a:= 0; a<=10)
            a := a+1;
            testing[a]:= a+1;
        end for;

        newProc(a,1);
        newProc1(a,2);
        return;
    end program
