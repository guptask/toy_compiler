program test is
	global integer test;
	global string aString;

	global procedure newProc(integer a in, bool b out)
	integer f[10];
	begin
		test[1+1] := 1;
		return;
	end procedure;

	begin
		aString := "Testing...";
		testing[1] := (a * -b) + c*d + f;

		// if statement
		if ( true ) then
			testing[2+a] := a/b;
		else
			testing[2] := a*b;
		end if;

		// for statement
		for (a:= 0; a<=10)
			a := a+1;
			testing[a]:= a+1;
		end for;

		newProc(a, 2);
		return;
	end program
