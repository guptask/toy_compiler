program aProgram is
    global integer testing[5];
    global string aString;
    integer a;
    integer b;
    integer c;
    integer d;
    float f;
    bool g;

    global procedure newProc(integer a in, integer b out)
        integer g[3];
        integer test;
        begin
            test := g[2+1]*b;
            return;
        end procedure;

    procedure newProc1(integer b out)
        integer g;
        begin
            newProc(g,b);
            newProc1(g);
            return;
        end procedure;

    begin
        getInteger(a);
        getBool(g);
        getFloat(f);
        putInteger(a);
        putBool(g);
        putFloat(f);
        aString := "Testing...";
        testing[1] := not g | (a * -b) + c*d + f/1;

        // if statement
        if ( true ) then
            testing[2+a] := a/b;
        else
            testing[2] := a*b;
        end if;

        // for statement
        for (a := 0; a<=10.1)
            a := a+1;
            testing[a] := a+1;
        end for;

        newProc(2, b);
        return;
    end program

